// models/dataModel.js

const mongoose = require('mongoose');

const ownerData = new mongoose.Schema({
  username: {
    type: String,
    required: true,
  },
  password: {
    type: String,
    required: true,
  },
  restaurant: {
    type: String,
    required: true,
  },
  description: {
    type: String,
  },
});

const ownerInfo = mongoose.model('ownerInfo', ownerData);

module.exports = ownerInfo;